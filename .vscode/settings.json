{
    "cSpell.enableFiletypes": [
        "mintyml"
    ],
    "[mintyml]": {
        "cSpell.ignoreRegExpList": [
            "\\[(\\s*\\S*=(\\S*|\"([^\"] | \\\\.)*\"|'([^'] | \\\\.)*'))*\\s*\\]",
            "[\\#.]\\S+",
            "(\\n|^)\\s*```[\\s\\S]*?\\n\\s*```",
            "(\\n|^)\\s*'''[\\s\\S]*?\\n\\s*'''",
            "(\\n|^)\\s*\"\"\"[\\s\\S]*?\\n\\s*\"\"\"",
            "<\\[\\#\\#\\[[\\s\\S]*?\\]\\#\\#\\]>",
            "<\\[\\#\\[[\\s\\S]*?\\]\\#\\]>",
            "<\\[\\[[\\s\\S]*?\\]\\]>",
            "<`[\\s\\S]*?`>",
        ]
    },
    "cSpell.ignoreRegExpList": [
        "\\{%.*%\\}",
        "\\{\\{.*%\\}\\}",
        "^---$[\\s\\S]*?^---$"
    ],
    "cSpell.words": [
        "strikethrough"
    ]
}